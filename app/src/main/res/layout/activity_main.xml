<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.MainActivity"
    android:gravity="center_horizontal"
    android:background="#4169E1">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <include android:id="@+id/camera"
                layout="@layout/layout_menu_item"/>

            <include android:id="@+id/audiocall"
                layout="@layout/layout_menu_item"/>

            <include android:id="@+id/filemanager"
                layout="@layout/layout_menu_item"/>

            <include android:id="@+id/clip"
                layout="@layout/layout_menu_item"/>

            <include android:id="@+id/webviewdownload"
                layout="@layout/layout_menu_item"/>

            <include android:id="@+id/fileoperation"
                layout="@layout/layout_menu_item"/>

            <include android:id="@+id/remotecontrol"
                layout="@layout/layout_menu_item"/>

            <include android:id="@+id/share"
                layout="@layout/layout_menu_item"/>

            <include android:id="@+id/notification"
                layout="@layout/layout_menu_item"/>

            <include android:id="@+id/call"
                layout="@layout/layout_menu_item"/>

            <include android:id="@+id/message"
                layout="@layout/layout_menu_item"/>

            <include android:id="@+id/watermark"
                layout="@layout/layout_menu_item"/>

            <include android:id="@+id/copyj"
                layout="@layout/layout_menu_item"/>


            <include android:id="@+id/gateway"
                layout="@layout/layout_menu_item"/>

            <include android:id="@+id/sandbox"
                layout="@layout/layout_menu_item"/>



            <Button
                android:id="@+id/test"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </LinearLayout>

    </ScrollView>



</LinearLayout>